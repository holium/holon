[
    {
        "process_name": "tester",
        "process_wasm_path": "/tester.wasm",
        "on_exit": "Restart",
        "request_networking": true,
        "request_messaging": [
            "net:sys:uqbar"
        ],
        "grant_messaging": [
            "vfs:sys:uqbar",
            {
                "process": "http_server:sys:uqbar",
                "params": {
                    "kind": "write",
                    "drive": "/tester:uqbar/tests"
                }
            }
        ],
        "public": true
    }
]

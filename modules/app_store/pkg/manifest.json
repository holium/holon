[
    {
        "process_name": "main",
        "process_wasm_path": "/app_store.wasm",
        "on_exit": "Restart",
        "request_networking": true,
        "request_messaging": [
            "terminal:terminal:uqbar",
            "filesystem:sys:uqbar",
            "http_server:sys:uqbar",
            "http_client:sys:uqbar",
            "encryptor:sys:uqbar",
            "net:sys:uqbar",
            "vfs:sys:uqbar",
            "kernel:sys:uqbar",
            "eth_rpc:sys:uqbar",
            {
                "process": "vfs:sys:uqbar",
                "params": {
                    "root": true
                }
            },
            {
                "process": "kernel:sys:uqbar",
                "params": "root"
            }
        ],
        "grant_messaging": [
            "http_server:sys:uqbar",
            "terminal:terminal:uqbar",
            "vfs:sys:uqbar"
        ],
        "public": false
    }
]